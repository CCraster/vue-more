{
	"name": "vue-more",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"online": "npm run build && git add . && git commit -m 'build & online' && git push",
		"lint": "vue-cli-service lint",
		"precommit": "lint-staged"
	},
	"dependencies": {
		"core-js": "^2.6.5",
		"vue": "^2.6.10",
		"vue-animejs": "^2.1.1",
		"vue-router": "^3.0.3",
		"vuex": "^3.0.1"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.11.0",
		"@vue/cli-plugin-eslint": "^3.11.0",
		"@vue/cli-service": "^3.11.0",
		"@vue/eslint-config-prettier": "^5.0.0",
		"babel-eslint": "^10.0.1",
		"eslint": "^5.16.0",
		"eslint-plugin-prettier": "^3.1.0",
		"eslint-plugin-vue": "^5.0.0",
		"husky": "^3.0.5",
		"less": "^3.0.4",
		"less-loader": "^5.0.0",
		"lint-staged": "^9.2.5",
		"prettier": "^1.18.2",
		"vue-template-compiler": "^2.6.10"
	},
	"lint-staged": {
		"src/**/*.{js,json,vue,css}": [
			"prettier --write",
			"vue-cli-service lint",
			"git add"
		]
	}
}
